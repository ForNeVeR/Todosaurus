<idea-plugin package="me.fornever.todosaurus.gitLab">
    <dependencies>
        <module name="me.fornever.todosaurus.core"/>
        <plugin id="com.intellij.tasks"/>
        <plugin id="org.jetbrains.plugins.gitlab"/>
    </dependencies>

    <resource-bundle>messages.TodosaurusGitLab</resource-bundle>

    <extensions defaultExtensionNs="me.fornever.todosaurus">
        <issueTrackerFactory implementation="me.fornever.todosaurus.gitLab.GitLabTrackerFactory"/>
        <issueOptionsFactory implementation="me.fornever.todosaurus.gitLab.ui.wizard.GitLabLabelsOptionsFactory"/>
    </extensions>
</idea-plugin>
