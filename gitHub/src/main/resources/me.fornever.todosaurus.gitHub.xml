<idea-plugin package="me.fornever.todosaurus.gitHub">
    <dependencies>
        <module name="me.fornever.todosaurus.core"/>
        <plugin id="com.intellij.tasks"/>
        <plugin id="org.jetbrains.plugins.github"/>
    </dependencies>

    <resource-bundle>messages.TodosaurusGitHub</resource-bundle>

    <extensions defaultExtensionNs="me.fornever.todosaurus">
        <issueTrackerFactory implementation="me.fornever.todosaurus.gitHub.GitHubTrackerFactory"/>
        <issueOptionsFactory implementation="me.fornever.todosaurus.gitHub.labels.ui.wizard.LabelsOptionsFactory"/>
    </extensions>
</idea-plugin>
